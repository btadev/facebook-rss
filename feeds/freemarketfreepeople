&#19981;&#21487;&#33021;&#30332;&#29983;&#65311;&#35731;&#25105;&#21578;&#35380;&#20320;&#19968;&#20491;&#27491;&#22312;&#30332;&#29983;&#30340;&#20363;&#23376;&#12290;&#31185;&#23416;&#30740;&#31350;&#22816;&#20013;&#31435;&#12289;&#23458;&#35264;&#65292;&#19981;&#33021;&#38568;&#20415;&#25140;&#24125;&#23376;&#65292;&#25630;&#25919;&#27835;&#39717;&#29229;&#20102;&#21543;&#65311;&#32016;&#32004;&#21644;&#21152;&#24030;&#30340;&#27298;&#23519;&#32317;&#38263;&#65292;&#32879;&#25163;&#35519;&#26597;&#30707;&#27833;&#24040;&#38957;&#33406;&#20811;&#26862;&#8231;&#32654;&#23386;&#26159;&#21542;&#22739;&#20303;&#23565;&#33406;&#20811;&#26862;&#19981;&#21033;&#30340;&#27683;&#20505;&#35722;&#36983;&#30740;&#31350;&#22577;&#21578;&#12290;&#27298;&#23519;&#32317;&#38263;&#20659;&#31080;&#20986;&#20358;&#65292;&#35201;&#30340;&#36039;&#26009;&#21487;&#22810;&#20102;&#65292;&#36001;&#21209;&#36039;&#26009;&#12289;&#38651;&#23376;&#37109;&#20214;&#21644;&#20854;&#23427;&#19968;&#22823;&#22534;&#25991;&#20214;&#65292;&#29992;&#30340;&#29702;&#30001;&#26159;&#33406;&#20811;&#26862;&#23565;&#25237;&#36039;&#20154;&#38577;&#30622;&#37325;&#35201;&#20844;&#21496;&#32147;&#29151;&#36039;&#35338;&#12290;&#20294;&#20840;&#19990;&#30028;&#37117;&#30693;&#36947;&#65292;&#27298;&#23519;&#32317;&#38263;&#26159;&#22312;&#25171;&#25802;&#24038;&#27966;&#30340;&#22823;&#25973;&#20154;&#65292;&#20840;&#29699;&#26262;&#21270;&#30340;&#26368;&#22823;&#20807;&#25163;&#12290;&#22914;&#26524;&#27492;&#38272;&#19968;&#38283;&#65292;&#20809;&#35380;&#35359;&#23601;&#21487;&#20197;&#24324;&#22446;&#19968;&#25171;&#30340;&#30707;&#27833;&#20844;&#21496;&#65292;&#25171;&#19981;&#36111;&#20063;&#27794;&#38364;&#20418;&#65292;&#33267;&#23569;&#20063;&#25226;&#33406;&#20811;&#26862;&#24324;&#24471;&#26356;&#33261;&#12290;&#12300;&#23565;&#25237;&#36039;&#20154;&#38577;&#30622;&#37325;&#35201;&#20844;&#21496;&#32147;&#29151;&#36039;&#35338;&#12301;&#26159;&#22810;&#40636;&#29702;&#30452;&#27683;&#22767;&